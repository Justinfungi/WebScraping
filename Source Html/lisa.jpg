                            <li class="product-review-pros-item"><%= statement.value %></li>
                        <% } );  %>
                    </ul>
                <% } %>
            </div>
        </div>
    </div>
</div>

	<% } %>
	<% if (description) { %>
		<div class="slideshow-slide-dek"><%= description %></div>
	<% } %>
</div>

			</div>

			<div class="slideshow-slide-product-nav">
				<button type="button" class="slide-button slide-button-previous" aria-label="Previous">
					<span class="icon icon-arrow-left02" aria-hidden="true"></span>
