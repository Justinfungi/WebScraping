<script type="text/template" id="slide-event-roundup-template">
	<% var count = id + 1 %>

	<div class="slideshow-slide slideshow-slide-<%= data.shape %> slideshow-slide-<%= data.type %>" id="slide-<%= id %>" data-id="<%= id %>">
		<div class="slide-media-outer" data-id="<%= id %>">
			<%= data.slideMediaView %>
		</div>

		<div class="slideshow-slide-event-content">
			<div class="slideshow-slide-event-body">
				<% if ( data.headline ) { %>
					<div class="slideshow-slide-hed"><%= data.headline %></div>
				<% } %>
				<% if ( data.photo_credit ) { %>
					<div class="slideshow-slide-credit"><%= data.photo_credit %></div>
				<% } %>
			</div>

			<% if (( data.credit || data.copyright ) && data.copyright !== "Hearst Owned" ) { %>
				<div class="image-credit slide-image-credit">
					<% if ( data.credit ) { %>
						<span class="image-photo-credit"><%= data.credit %></span>
					<% } %>

