				<% if (data.customTag) { %>
					<div class="slideshow-slide-product-review-label"><%= data.customTag %></div>
				<% } %>
				<%
	var name = data.product.name;
	var brand = data.product.product.brand;
	var description = data.product.description;
	var product = data.product;
	var vendor = data.product.vendor;
	var pros = data.product.pros;
	var cons = data.product.cons;
%>
<div class="product-slide-content">
	<% if (name) { %>
		<div class="slideshow-slide-hed"><%= name %></div>
	<% } %>
	<div class="product-slide-details noskim">
		<% if (brand) { %>
			<span class="product-slide-brand"><%= brand %></span>
		<% } %>
		<% if (vendor) { %>
			<span class="product-slide-vendor"><%= vendor %></span>
		<% } %>
		<% var itemOnSale = product.item_on_sale && !0; %>
<% var listPrice = product.list_price %>
<% var price = product.price %>
<% var discount = product.discount %>
<% if (product.show_price && product.priceNoSymbol > 0) { %>
	<span class="product-slide-price">
		<% if (itemOnSale) { %>
			<span aria-label="Original Price " tabindex="0" class="list-price text-strike"><%= listPrice %></span>
			<div aria-label="Sale Price   percent off" tabindex="0" class="discount-price text-bold">
				<%= price %> (<%= discount %>% off)
			</div>
		<% } else { %>
			<%= price %>
		<% } %>
	</span>
<% } %>

	</div>
	<div class="product-buy-button-wrapper">
	

<%
	var product = product
	var brand = product.product.brand
	var cta = product.action || "SHOP NOW"
	var href = product.retailer_url
	var outboundLink = product.outboundLink
	var item_on_sale = product.item_on_sale
	var productBrand = product.brand || brand
	var productPrice = product.price
	var sem3Id = product.product.sem3_id
	var productId = product.id
	var retailerSku = product.retailer.sku
	var affiliateUrl = product.retailer.affiliate_url || ""
	var retailerAffiliate = product.retailer.affiliate

	if (product.retailer.url && !product.retailer_url) {
		href = product.retailer.getFrontendUrl
	}

	var isBlockAffiliates = product.metadata.is_block_affiliate > 0
	var useAffiliateUrlFlag = false

	if (useAffiliateUrlFlag && product.retailer.affiliate_url && !isBlockAffiliates) {
		href = affiliateUrl
	}
	if (!useAffiliateUrlFlag) {
		affiliateUrl = ""
		affiliateNetwork = ""
	}

	var newProductStylingFlag = false
	var isArbTraffic = false

	if (isArbTraffic) {
		cta = "Check Price"
	}

	if (newProductStylingFlag) {
		var productRetailer = typeof getRetailerName !== 'undefined' ? getRetailerName(product) : product.vendor
		if (product.show_price && !isArbTraffic) {
	        var price = Math.round(parseFloat(product.priceNoSymbol) || 0)
			productPrice = product.price.replace(product.priceNoSymbol, price)
			if (!price) {
				productPrice = "Shop"
			}
		} else {
			productPrice = "Shop"
		}

		if (productRetailer && !product.action) {
			cta = "%PRICE% at %RETAILER%".replace('%PRICE%', productPrice).replace('%RETAILER%', productRetailer).toUpperCase()
		}
	}

	var classes = []
	if (product.is_multi_retailer_product) {
		classes.push('product-btn-link-multi-retailer')
	}
	if (isBlockAffiliates) {
		classes.push('noskim')
		classes.push('noamzn')
	}
	classes = classes.join(' ')
%>

<% var role = data.role %>
<% var link_treatment = item_on_sale ? "sale" : "" %>

<a class="product-btn-link <%= classes %>"
	href="<%= href %>"
	data-href="<%= href %>"
	data-vars-ga-call-to-action="<%= cta %>"
			data-vars-ga-link-treatment="<%= link_treatment %>"
	    data-affiliate-url="<%= affiliateUrl %>"
	data-affiliate-network="<%= _.escape(JSON.stringify(retailerAffiliate)) %>"
	data-vars-ga-media-role="<%= role %>"
	data-vars-ga-outbound-link="<%= outboundLink %>"
	data-vars-ga-product-brand="<%= productBrand %>"
	data-vars-ga-product-id="<%= productId %>"
	data-vars-ga-product-price="<%= productPrice %>"
	data-vars-ga-product-sem3-brand="<%= brand %>"
	data-vars-ga-product-sem3-category="<%= product.category %>"
	data-vars-ga-product-sem3-id="<%= sem3Id %>"
	data-vars-ga-media-type="product_review"
	data-vars-ga-ux-element="Product Button"
		data-vars-ga-magento-tracking="1"
	target="_blank"
	rel="nofollow"><%= cta %> <%= product.call_to_action %></a>

</div>

	<% if (!_.isEmpty(pros) || !_.isEmpty(cons)) { %>
